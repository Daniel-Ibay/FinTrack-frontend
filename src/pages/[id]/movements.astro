---
import LayoutUserApplication from "@/layouts/LayoutUserApplication.astro";
import MovementsSection from "@/components/app-user/movements.jsx"
import type { GetStaticPaths } from "astro";

export const getStaticPaths: GetStaticPaths = (() => {
	
	return [
		{params: {id: '67cea80f2c143f352d4d7d22'}}
	];
}) satisfies GetStaticPaths;

const { id } = Astro.params
---
<LayoutUserApplication>
	<MovementsSection client:load id={id}/>
</LayoutUserApplication>
